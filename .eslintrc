{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "strict": 0,
    "no-multi-spaces": 0,
    "quotes": [
      1,
      "single"
    ],
    "no-shadow": 0,
    "no-empty": 0,
    "no-cond-assign": 0,
    "no-loop-func": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0
  }
}
